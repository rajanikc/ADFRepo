{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "AzureSqlDatabase05",
						"type": "LinkedServiceReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     pSrcTblName as string ('EMPLOYEE_LIST'),",
				"     pLastUpdTmstamp as string ('1900-01-01'),",
				"     pDeltaFlag as string ('N'),",
				"     parameter1 as string (iif($pDeltaFlag == 'N', '1900-01-0', $pLastUpdTmstamp))",
				"}",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     format: 'query',",
				"     store: 'sqlserver',",
				"     query: (@concat('SELECT * FROM DBO.', $pSrcTblName,'WHERE ETL_LAST_'),",
				"     isolationLevel: 'READ_UNCOMMITTED') ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          idnumber as string,",
				"          designation as string,",
				"          ETL_CREATE_TIMESTAMP as string,",
				"          ETL_UPDATE_TIMESTAMP as string,",
				"          ETL_ACTIVE_FLAG as string",
				"     ),",
				"     partitionFileNames:['Employee_list.txt'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}