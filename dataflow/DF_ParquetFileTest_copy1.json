{
	"name": "DF_ParquetFileTest_copy1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "LSAzureDataLakeStorage",
						"type": "LinkedServiceReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet',",
				"     fileSystem: 'adlscontainer',",
				"     folderPath: 'InputFilesDirectory',",
				"     fileName: 'Employees_list.parquet') ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          IdNumber as string,",
				"          EmployeeFirstName as string,",
				"          EmployeeLastName as string,",
				"          updatedate as string",
				"     ),",
				"     partitionFileNames:['Employees_list1.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1),",
				"     format: 'parquet',",
				"     preCommands: [],",
				"     postCommands: []) ~> sink1"
			]
		}
	}
}